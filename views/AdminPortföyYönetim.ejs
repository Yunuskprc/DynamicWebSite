        <%- include('./partials/AdminSideBar') %>

            <div class="b-example-divider"></div>

            <div class="iletisimPanel" style="width: 83%;margin-left: 21%; margin-top: -49%; height: 900px;">
                <div style="width: 94%;height: 100%;border: 1px solid #dee2e6 ;border-radius: 10px;">

                    <div style="border: 1px solid #dee2e6;border-radius: 10px;margin: 1%;">
                        <figure class="text-center">
                            <blockquote class="blockquote">
                                <p>Seni ziyaret edenlere kendinden bahsetmeye ne dersin?</p>
                            </blockquote>
                            <figcaption class="blockquote-footer">
                                Ziyaretçilerinin ilgisini çekecek konular ekle! <cite title=""></cite>
                            </figcaption>
                        </figure>
                    </div>

                    <div
                        style="border: 1px solid #dee2e6;border-radius: 10px;margin: 1%;width: 700px;justify-content: center;text-align: center;">
                        <figure class="text-">
                            <blockquote class="blockquote" style="font-size: 100%;">
                                <p>Bu kısma ismini,soyismini yaz.Kısaca insanlara kendinden bahset.</p>
                            </blockquote>
                            <figcaption class="blockquote-footer">
                                Neden senin siteni ziyaret etmeliler? <cite title=""></cite>
                            </figcaption>
                        </figure>
                    </div>

                    <div class="input-group" style="padding: 1%;">
                        <span class="input-group-text">İsim ve soyismin nedir?</span>
                        <input type="text" aria-label="First name" class="form-control">
                        <input type="text" aria-label="Last name" class="form-control">
                    </div>

                    <div class="input-group mb-3" style="padding: 1%;">
                        <span class="input-group-text" id="inputGroup-sizing-sm">Hangi şehirde yaşıyorsun?</span>
                        <input type="text" class="form-control" aria-label="Sizing example input"
                            aria-describedby="inputGroup-sizing-sm">
                    </div>

                    <div class="bd-example m-0 border-0" style="padding: 1%;">
                        <div class="form-floating">
                            <textarea class="form-control" placeholder="Leave a comment here" id="floatingTextarea2"
                                style="height: 300px"></textarea>
                            <label for="floatingTextarea2">Biyografin</label>
                        </div>
                    </div>

                    <div class="input-group mb-3" style="padding: 1%;">
                        <span class="input-group-text" id="inputGroup-sizing-sm">Hangi üniversiteden mezun oldun?</span>
                        <input type="text" class="form-control" aria-label="Sizing example input"
