    <style>

    .navBar{
      height: 200px;
    }

    .link-container {
      display: flex;
      justify-content: space-around;
      margin-top: 3.5%;
    }

    .link-container a {
      position: relative;
      text-decoration: none;
      color: #333;
      /* Altı çizgi efekti için 5 saniyelik geçiş süresi */
    }

    .link-container a::after {
      content: '';
      position: absolute;
      left: 0;
      bottom: -3px;
      width: 100%;
      height: 1px;
      background-color: #333;
      /* Alt çizgi rengini istediğiniz renkle değiştirebilirsiniz */
      transform: scaleX(0);
      transition: transform 0.3s ease-in-out;
    }
    .link-container a:hover::after {
      transform: scaleX(1);
    }



    .link-container a+a {
      margin-left: -50%;
    }

    .kategori{
      margin-top: 70px;
      margin-bottom: 30px;
      display: flex;
      justify-content: space-around;
    }

    .kategori a{
      position: relative;
      text-decoration: none;
      color: #333;
      margin: 0 100px;
    }
    .kategori a::after {
      content: '';
      position: absolute;
      left: 0;
      bottom: -3px;
      width: 100%;
      height: 1px;
      background-color: #333;
      transform: scaleX(0);
      transition: transform 0.3s ease-in-out;
    }
    .kategori a:hover::after {
      transform: scaleX(1);
    }
    .kategori a+a {
      margin-left: -50%;
    }

    #carouselExampleCaptions:hover .carousel-inner .carousel-item img {
    filter: blur(2px); 
    transition: filter 0.3s ease;
    }
    #carouselExampleCaptions .carousel-inner .carousel-item .carousel-caption h5,
    #carouselExampleCaptions .carousel-inner .carousel-item .carousel-caption p {
        display: none; 
        transition: filter 1s ease;
    }

    #carouselExampleCaptions:hover .carousel-inner .carousel-item .carousel-caption h5,
    #carouselExampleCaptions:hover .carousel-inner .carousel-item .carousel-caption p {
        display: block;      }

    
    .nav-link {
      position: relative;
      text-decoration: none;
      color: #6c757d;
      display: inline-block;
      margin-bottom: 0px;
    }

    .nav-link::after {
      content: '';
      position: absolute;
      left: 0;
      bottom: 3px;
