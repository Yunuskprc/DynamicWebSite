  <script>

function sendLocaiton(degisken,projeAd) {
  console.log(degisken);
  fetch('/api/Projelerimiz/KategoriAdi', {
    method: 'POST',
    headers: {
      'Content-Type': 'application/json',
    },
    body: JSON.stringify({ degisken: degisken }), // Değişkeni JSON formatına çevir
  })
  .then(response => response.json())
  .then(data => {
      console.log('Sunucudan gelen veri:', data);
      window.location.href = `/Projelerimiz/${data.data}/${projeAd}`
      // İşlemlerinizi burada yapabilirsiniz
  })
  .catch(error => {
      console.error('Hata:', error);
  });
}


  </script>
